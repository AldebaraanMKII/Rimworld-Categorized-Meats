<?xml version="1.0" encoding="utf-8" ?>

<Patch>


<Operation Class="PatchOperationFindMod">
    <mods>
        <li>Vanilla Animals Expanded â€” Royal Animals</li>
    </mods>
	<match Class="PatchOperationSequence">
	<operations>
	
	
   <li Class="PatchOperationReplace">
	  <xpath>Defs/ThingDef[
	  	  defName="VAERoy_Orangutan" 
	  ]/race/useMeatFrom</xpath>
      <value>
         <useMeatFrom>Monkey</useMeatFrom>
      </value>
   </li>
	

	
   <li Class="PatchOperationReplace">
	  <xpath>Defs/ThingDef[
	  	  defName="VAERoy_Peacock" or
	  	  defName="VAERoy_Pheasant" 
	  ]/race/useMeatFrom</xpath>
      <value>
         <useMeatFrom>Chicken</useMeatFrom>
      </value>
   </li>
	

	
   <li Class="PatchOperationReplace">
	  <xpath>Defs/ThingDef[
	  	  defName="VAERoy_RoyalTiger" 
	  ]/race/useMeatFrom</xpath>
      <value>
         <useMeatFrom>Cat</useMeatFrom>
      </value>
   </li>
	



	
	</operations>
	</match>
</Operation>
</Patch>